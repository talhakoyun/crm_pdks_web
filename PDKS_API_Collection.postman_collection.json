{
  "info": {
    "name": "PDKS API Koleksiyonu",
    "description": "PDKS (Personel Devam Kontrol Sistemi) API Koleksiyonu",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Auth",
      "description": "Kimlik doğrulama işlemleri için API endpoint'leri",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"email\": \"kullanici@ornek.com\",\n    \"password\": \"sifre123\",\n    \"device_id\": \"device_123456\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/login",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "login"]
            },
            "description": "Kullanıcı girişi yapar ve JWT token döndürür.\n\nGerekli Parametreler:\n- email: Kullanıcı e-posta adresi\n- password: Kullanıcı şifresi\n- device_id: Cihaz kimliği\n\nDönüş Değeri:\n- access_token: JWT token\n- token_type: Token tipi (bearer)\n- expires_in: Token geçerlilik süresi (saniye)"
          },
          "response": []
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/auth/refresh",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "refresh"]
            },
            "description": "JWT token'ı yeniler.\n\nDönüş Değeri:\n- access_token: Yeni JWT token\n- token_type: Token tipi (bearer)\n- expires_in: Token geçerlilik süresi (saniye)"
          },
          "response": []
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/auth/logout",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "logout"]
            },
            "description": "Kullanıcı çıkışı yapar ve token'ı geçersiz kılar.\n\nDönüş Değeri:\n- status: İşlem durumu\n- message: İşlem mesajı"
          },
          "response": []
        },
        {
          "name": "Me",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept",
                "value": "application/json"
              },
              {
                "key": "Authorization",
